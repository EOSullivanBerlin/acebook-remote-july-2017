<ul class='users-index-friends'>
    <% @user_friends.each do |friend| %>
      <li class='users-index-friend'>
        <section class='friends-index-horizontal'>
          <p class='users-index-friend-icon'>
            <%= image_tag("user_icon.png", :alt => "user icon") %>
          </p>
          <p class="users-index-friend-name">
            <%= user.first_name + " " + user.last_name %>
          </p>
        </section>
    <% end %>
</ul>
